@model IEnumerable<string>
@{ 
    ViewBag.Title = "Тестовое WebAPI";
}

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @{ var i = 1; }
            @foreach (var s in Model)
            {
                <tr>
                    <td>@(i++)</td>
                    <td>@s</td>
                </tr>

            }
        </tbody>

    </table>
</div>