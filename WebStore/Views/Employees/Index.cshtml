@model IEnumerable<WebStore.Models.Employee>

<div class="container">
    <h3>Сотрудники</h3>
    <hr />

    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.LastName</td>
                <td>@item.FirstName</td>
                <td>@item.Patronymic</td>
                <td>@item.Age</td>
            </tr>
            }
        </tbody>
    </table>

    <p><a asp-controller="Home" class="btn btn-default">Домой</a></p>
</div>
